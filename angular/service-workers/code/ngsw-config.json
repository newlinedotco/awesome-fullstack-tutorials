{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": ["/index.html", "/favicon.ico", "/*.css", "/*.js"]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "lazy",
            "resources": {
                "files": ["/assets/**"],
                "urls": ["https://angular.io/assets/images/logos/angular/angular.svg"]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "random.org",
            "urls": ["https://www.random.org/**"],
            "cacheConfig": {
                "maxSize": 3,
                "maxAge": "7d",
                "strategy": "freshness"
            }
        }
    ]
}
